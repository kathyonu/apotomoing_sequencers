<h2>Complete sequence search results</h2>

  <%= "your processed entry ::: " + @sequencetextdecommaed %><br />
  <%= "its complete sequence :: " + @complete_sequence %><br />
  <%= "total matches found ::::: " + @completes_count %><br /><br /> 

<div id="resultcomplete">
  <table>
    <tr>
      <th>Complete Matches</th>
      <th></th>
      <th></th>
    </tr>

    <% @sequences.each do |sequence| %>
      <tr>
        <td><%= sequence.sequence_text %></td>
        <td><%= link_to 'Show', sequence %></td>
        <td><%= link_to 'Edit', edit_sequence_path(sequence) %></td>
      </tr>
    <% end %>
  </table>
</div>

